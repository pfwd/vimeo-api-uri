{
    "name": "pfwd/vimeo-api-uri",
    "description": "A PHP Library that holds Vimeo API URI's",
    "type": "library",
    "keywords": ["vimeo","api", "uri"],
    "homepage": "https://github.com/pfwd/vimeo-api-uri",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Fisher",
            "homepage": "http://peterfisher.me.uk",
            "role": "Developer"
        }
    ],
    "suggest": {
        "pfwd/vimeo-entities": "A set of PHP entities that represent responses from the Vimeo API"
    },
    "autoload": {
        "psr-4": {
            "PFWD\\Vimeo\\": "src/Vimeo/"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.3",
        "codeception/codeception": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    }
}
